<!DOCTYPE html>
<html>
  <head>
    <title>Illustofu</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header class="header">
      <div class="nav">
        <div class="nav_left">
          <%= link_to image_tag("いらすとーふ.png",class: :logo),root_path%>
        </div>
      </div>
    </header>
    <%= yield %>
  </body>
</html>
